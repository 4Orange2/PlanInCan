  G4P.setGlobalColorScheme(GCScheme.BLUE_SCHEME);
  G4P.setMouseOverEnabled(false);
  surface.setTitle("Sketch Window");
  Home_Page = GWindow.getWindow(this, "Window title", 0, 0, 1000, 550, JAVA2D);
  Home_Page.noLoop();
  Home_Page.setActionOnClose(G4P.CLOSE_WINDOW);
  Home_Page.addDrawHandler(this, "win_draw1");
  button4 = new GButton(Home_Page, 327, 210, 92, 30);
  button4.setText("Saved Plans");
  button4.addEventHandler(this, "button4_click1");
  button5 = new GButton(Home_Page, 328, 149, 93, 30);
  button5.setText("Recommended Plans");
  button5.addEventHandler(this, "button5_click1");
