  button6.setText("Start a New Plan");
  button6.addEventHandler(this, "button6_click1");
  Recommended_Page = GWindow.getWindow(this, "Window title", 0, 0, 1000, 550, JAVA2D);
  Recommended_Page.noLoop();
  Recommended_Page.setActionOnClose(G4P.KEEP_OPEN);
  Recommended_Page.addDrawHandler(this, "win_draw3");
  Saved_Plans = GWindow.getWindow(this, "Window title", 0, 0, 1000, 550, JAVA2D);
  Saved_Plans.noLoop();
  Saved_Plans.setActionOnClose(G4P.KEEP_OPEN);
  Saved_Plans.addDrawHandler(this, "win_draw4");
  New_Plan_Page = GWindow.getWindow(this, "Window title", 0, 0, 1000, 550, JAVA2D);
  New_Plan_Page.noLoop();
